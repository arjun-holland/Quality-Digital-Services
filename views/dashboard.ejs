body {
      margin: 0;
      padding-top: 0px;
      font-family: "Poppins", sans-serif;
    }
     header {
      background-color: #fff;
      padding: 10px 40px;
      display: flex;
      justify-content: space-between;
      align-items: center;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }

    .logo {
      display: flex;
      align-items: center;
    }

    .logo img {
      width: 400px;
      height: 100px;
      margin-right: 10px;
    }

    .logo span {
      font-size: 20px;
    }

    .logo span b {
      color: #f90;
    }

    nav a {
      margin-left: 30px;
      text-decoration: none;
      color: #ff6600;
      font-weight: bold;
      font-size: 20px;
    }

    .container {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      max-width:100%;
      height: 698px; /* increase as needed */
      /* margin: 20px; */
      background: linear-gradient(to right, #001f3f, #003366);
      color: white;
      padding: 17px 10px;
    }

      
    h2 {
      padding-top: 40px;
      text-align: center;
      color: black;
      font-size: 35px;
      margin-left: 70px;
    } 
    .highlight {
      color: #ff6600;
      font-weight: bold;
    }  
    .card {
  background-color: white;
  color: #333;
  border-radius: 20px;
  margin: 10px;
  padding: 20px;
  width: 300px;
  height: 200px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);

  display: flex;
  flex-direction: column;
  align-items: center;    /* Horizontal center */
  justify-content: center; /* Vertical center */
  text-align: center;
}

.card img {
  width: 70px;
  height: 70px;
  margin-bottom: 10px; /* spacing below the image */
}
.grid-container {
      display: grid;
      grid-template-columns: repeat(auto-fill, minmax(130px, 1fr));
      gap: 30px;
      padding: 20px;
      max-width: 1100px;
      margin: auto;
      font-size: 19px;
      color: #003366;
    }
    .grid-item {
      text-align: center;
      font-size: 19px;
      font-weight: bold;
      display: inline-block;
      overflow: hidden;
      border-radius: 30px;
    }
    .grid-item img {
      width: 150px;
      height: 150px;
      object-fit: contain;
      margin-bottom: 8px;
      border-radius: 30px;
    }
    h3{
      font-weight: bold;
    }
    .footer {
      background-color: #06113C;
      color: #fff;
      padding: 40px 20px 20px;
    }

    .footer-container {
      max-width: 1200px;
      margin: 0 auto;
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
    }

    .footer-column {
      flex: 1;
      min-width: 180px;
      margin: 10px;
    }

    .footer-column h3 {
      margin-bottom: 15px;
      color: #fff;
      font-size: 18px;
    }

    .footer-column a {
      display: block;
      color: #ccc;
      text-decoration: none;
      margin: 8px 0;
      font-size: 14px;
      transition: color 0.3s;
    }

    .footer-column a:hover {
      color: #fff;
    }

    .newsletter input[type="email"] {
      padding: 8px;
      border: none;
      border-radius: 4px;
      width: 70%;
      margin-right: 5px;
    }

    .newsletter button {
      padding: 8px 16px;
      background-color: #E8772E;
      color: #fff;
      border: none;
      border-radius: 4px;
      cursor: pointer;
      transition: background-color 0.3s;
    }

    .newsletter button:hover {
      background-color: #c96220;
    }

    .social-icons {
      margin-top: 10px;
    }

    .social-icons img {
      width: 30px;
      height: 30px;
      margin: 0 5px;
      vertical-align: middle;
      border-radius: 4px;
    }

    .footer-bottom {
      border-top: 1px solid #ccc;
      margin-top: 10px;
      padding-top: 10px;
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-wrap: wrap;
      background-color: #ccc;
    }

    .footer-logo img {
      height: 80px;
      width: 300px;
    } 

    .footer-copy {
      font-size: 13px;
      color: #ccc;
    }
.card {
  cursor: pointer;
  transition: transform 0.2s ease;
}

.card:hover {
  transform: scale(1.05);
}


.grid-item {
  cursor: pointer;
  transition: transform 0.7s cubic-bezier(0.50, 1, 0.5, 1);
}

.grid-item:hover {
  transform: scale(1.15);
}



a {
  color: inherit;
  text-decoration: none;
}

