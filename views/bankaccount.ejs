/* Individual loan/credit card */
.loan-card {
  background: white;
  border-radius: 20px;
  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);
  padding: 20px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  transition: transform 0.2s ease;
}

.loan-card:hover {
  transform: translateY(-5px);
}

/* Header with logo and text */
.loan-card-header {
  display: flex;
  align-items: center;
  margin-bottom: 12px;
}

.loan-card-header img {
