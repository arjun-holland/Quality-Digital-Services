  color: #c60000;
}

.info-card h3 {
  margin-top: 20px;
  color: #333;
}

.info-card ul, .info-card ol {
  padding-left: 20px;
  margin-top: 10px;
}

.app-logo {
  width: 60px;
  margin-bottom: 10px;
}

.form-card {
  flex: 1;
  background-color: white;
  padding: 20px;
  border-radius: 10px;
  border: 1px solid #ccc;
  box-shadow: 2px 2px 8px rgba(0,0,0,0.1);
}

.form-card h2 {
  margin-bottom: 20px;
  text-align: center;
}

form {
  display: flex;
  flex-direction: column;
}

label {
  margin: 10px 0 5px;
  font-weight: bold;
}

input, select {
  padding: 10px;
  border-radius: 5px;
  border: 1px solid #aaa;
  margin-bottom: 10px;
}

button {
  padding: 12px;
  background-color: #f57c00;
  color: white;
  border: none;
  border-radius: 5px;
  font-weight: bold;
  cursor: pointer;
  transition: background 0.3s;
}

button:hover {
  background-color: #e06b00;
}
</style>
</head>
