}

.nav-links a {
  margin: 0 10px;
  text-decoration: none;
  color: #f57c00;
  font-weight: 500;
}

.main-container {
  display: flex;
  gap: 20px;
  margin-top: 30px;
  flex-wrap: wrap;
}

.info-card {
  flex: 2;
  background-color: #e8f1ff;
  padding: 20px;
  border-radius: 10px;
  box-shadow: 0 0 8px rgba(0,0,0,0.1);
}

.info-card h2 {
