/* Heading aligned to grid */
.section-title {
  font-size: 37px;
  font-weight: 800;
  font-family: 'Segoe UI', sans-serif;
  color: #000;
  margin-bottom: 30px;
    text-align: center;
}

.sub-text {
  color: #ff8c1a;
  font-weight: 600;
  font-size: 22px;
  margin-left: 10px;
}


/* Grid layout for cards */
.cards-container {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
}


.cards-container {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
  padding: 30px;
  max-width: 1200px;
  margin: auto;
  background-color: #f1f5ff;
}

/* Individual loan/credit card */
.loan-card {
  background: white;
  border-radius: 20px;
  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);
  padding: 20px;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  width: 100%;
  transition: transform 0.2s ease;
}

.loan-card:hover {
  transform: translateY(-5px);
}

/* Header with logo and text */
.loan-card-header {
  display: flex;
  align-items: center;
  margin-bottom: 12px;
}

.loan-card-header img {
  width: 45px;
  height: 45px;
  margin-right: 10px;
}

.loan-card-info h3 {
  margin: 0;
  font-size: 17px;
}

.loan-card-info p {
  margin: 0;
  font-size: 14px;
  color: #444;
}

/* Offer banner */
.offer-banner {
  background-color: #ff8c1a;
  color: white;
  text-align: center;
  padding: 10px;
  border-radius: 8px;
  margin: 15px 0;
  font-weight: bold;
  font-size: 15px;
}

/* Fees box */
.fees-section {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #bfeaff;
  border-radius: 6px;
  padding: 10px;
  margin-bottom: 15px;
}

.fee-box {
  text-align: center;
  flex: 1;
  font-weight: bold;
  color: #222;
}

.fee-divider {
  font-size: 20px;
  color: #555;
  margin: 0 8px;
}

/* Features */
.features {
  list-style: none;
  padding: 0;
  margin: 10px 0;
  color: #333;
  font-size: 14px;
}

.features li {
  margin-bottom: 5px;
}

/* Buttons */
.buttons {
  display: flex;
  justify-content: space-between;
  gap: 10px;
  margin-top: 10px;
}

.buttons button {
  background-color: #ff8c1a;
  color: white;
  border: none;
  border-radius: 8px;
  padding: 10px 15px;
  font-weight: bold;
  cursor: pointer;
  flex: 1;
  font-size: 12px;
  transition: background-color 0.2s ease;
}

.buttons button:hover {
  background-color: #e67300;
}
